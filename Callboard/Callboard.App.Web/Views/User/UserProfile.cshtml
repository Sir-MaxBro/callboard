    <div class="center">
        <a href="@Url.Action("CreateAdDetails", "AdDetails")" class="waves-effect waves-light btn">
            <i class="material-icons left">add</i>
            Create ad
        </a>
    </div>
</div>
<div class="width_75per float_right">
    @{ Html.RenderAction("GetAdsForUser", "Ad", new { userId = Model.UserId }); }
</div>