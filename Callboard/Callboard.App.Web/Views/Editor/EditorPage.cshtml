@Scripts.Render("~/editor")

<div class="row">
    <div class="col s6">
        @{ Html.RenderPartial("../Kind/KindsEdit"); }
    </div>
    <div class="col s6">
        @{ Html.RenderPartial("../State/StatesEdit"); }
    </div>
</div>

<div class="row">
    <div class="col s4">
        @{  Html.RenderPartial("../Country/CountriesEdit"); }
    </div>
    <div class="col s4">
        @{  Html.RenderPartial("../Area/AreasEdit"); }
    </div>
    <div class="col s4">
        @{  Html.RenderPartial("../City/CitiesEdit"); }
    </div>
</div>

<div class="row card">
    <div class="card-content">
        <span class="card-title">Categories</span>
        <div id="categories">
            @{ Html.RenderAction("GetEditCategories", "Category"); }
        </div>
    </div>
    <div class="card-action">
        <button class="btn waves-effect waves-light green" type="button" onclick="createCategory('categories')">
            Add category
            <i class="material-icons left">add</i>
        </button>
    </div>
</div>