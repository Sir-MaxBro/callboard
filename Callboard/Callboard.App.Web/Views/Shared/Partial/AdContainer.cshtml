@model IReadOnlyCollection<Ad>
<script>
    $(document).ready(function () {
        $('.carousel').carousel();
    });
</script>

<div class="row">
    @if (Model?.Count <= 0)
    {
        Html.RenderPartial("Partial\\NothingFound");
    }

    @foreach (var item in Model)
    {
        <div class="col m6 l4">
            @{ Html.RenderPartial("Partial\\AdPartial", model: item); }
        </div>
    }
</div>